mendeley-groups = {vkr_articles},
pages = {659--664},
title = {{Position/force control for robot manipulators applying robust control algorithms}},
volume = {2},
year = {1999}
}

@article{indirect_to_direct,
abstract = {The leading industrial robotics community seems to be ready for a big step in designing enhanced control systemss namely, the integration of force control capabilities. This paper presents a roadmap for eeective realization of such a step where the key knots from motion control to interaction control are enlightened. In the framework of model-based task space control, compliance control and impedance control are introduced as the basic strategies for indirect force control. Then, direct force control strategies are considered which are obtained from motion control schemes suitably modiied by the closure of an outer force regulation feedback l o o p . A k ey feature is the coverage of six-degree-of-freedom interaction tasks.},
author = {Siciliano, Bruno and Villani, Luigi and Federico, Napoli},
journal = {Robotica},
mendeley-groups = {vkr_articles},
number = {May 2014},
pages = {1--36},
title = {{From Indirect to Direct Force Control : A Roadmap for Enhanced Industrial Robots}},
year = {2000}
}

