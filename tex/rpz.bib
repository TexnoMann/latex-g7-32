journal = {Automatika},
keywords = {Cooperative manipulators,centralized control,force/position control,interaction dynamics,kinematics},
mendeley-groups = {vkr_articles},
number = {1},
pages = {113--123},
title = {{Model-based force/position control of cooperative manipulation systems}},
url = {https://doi.org/10.1080/00051144.2019.1580853},
volume = {60},
